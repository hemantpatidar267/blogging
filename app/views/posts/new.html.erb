<h1>Post Create</h1>
<p><%= flash[:notice] %></p>
<%= form_with model: @post, html: { multipart: true } do |form| %>

  <div>
    <%= form.label :Tittle%><br>
    <%= form.text_field :tittle %>
  </div>

  <div>
    <%= form.label :Context %><br>
    <%= form.text_field :context %>
  </div>
  <div>
    <%= form.label :blog_pic %>
    <%= form.file_field :blog_pic %>
  </div>

  <div>
    <%= form.submit %>
  </div>

<% end %>

<%= button_to "Sign out", destroy_user_session_path, method: :delete %>

