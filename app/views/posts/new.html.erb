<h1>Post Create</h1>

<%= form_with model: @post do |form| %>

  <div>
    <%= form.label :Tittle%><br>
    <%= form.text_field :tittle %>
  </div>

  <div>
    <%= form.label :Context %><br>
    <%= form.text_field :context %>
  </div>

  <div>
    <%= form.submit %>
  </div>

<% end %>

<%= button_to "Sign out", destroy_user_session_path, method: :delete %>
<%= button_to "create", new_post_path, method: :get %>
